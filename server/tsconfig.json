{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "rootDir": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@db/*": ["./src/db/*"],
      "@server/*": ["./src/app/*"]
    }
  }
}
